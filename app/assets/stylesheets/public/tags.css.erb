	background-image: url(<%= asset_path 'navigation/control_start_blueIA.png' %>);
	background-repeat: no-repeat;
  background-position: 0.5% 50%;
	font-size:1em;
	padding:2px 2px 2px 18px;
	color:#9F9F9F;
}

#main .prevSetIA{
	background-image: url(<%= asset_path 'navigation/control_rewind_blueIA.png' %>);
	background-repeat: no-repeat;
  background-position: 0.5% 50%;
	font-size:1em;
	padding:2px 2px 2px 18px;
	color:#9F9F9F;
}

#main .lastIA{
	background-image: url(<%= asset_path 'navigation/control_end_blueIA.png' %>);
	background-repeat: no-repeat;
  background-position: 100% 50%;
	font-size:1em;
	padding:2px 18px 2px 2px;
	color:#9F9F9F;
}

#main .nextSetIA{
	background-image: url(<%= asset_path 'navigation/control_fastforward_blueIA.png' %>);
	background-repeat: no-repeat;
  background-position: 100% 50%;
	font-size:1em;
	padding:2px 18px 2px 2px;
	color:#9F9F9F;
}

.ext_link, .ext_link:visited, .help_ext_link, .help_ext_link:visited{
	color:#1B5EDF;
	padding-right:15px;
	background: transparent url(<%= asset_path 'css/ext_link.png' %>) center no-repeat;
	background-position:100% 50%
}

.help_ext_link{
  background: transparent url(<%= asset_path 'css/icon_info.gif' %>) center no-repeat;
  background-position:100% 50%
}

.clear{ clear: both !important;  }
.no_margin{margin: 0px !important;}
.side_margin{margin-left: 5%; margin-right: 5%;}
.i_side_margin{margin-left: 5% !important; margin-right: 5% !important;}
.y_padding10{padding-top: 10px; padding-bottom: 10px;}
.top_padding10{padding-top: 10px;}
.no_decorators{border:none !important; text-decoration: initial !important; font-style:normal !important;}
.normal_list{ display:list-item !important; }
.block{display: block !important;}