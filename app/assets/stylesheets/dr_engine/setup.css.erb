#advanced_options_head, #rothstein_sdl_head{font-size:19px;font-variant:small-caps;color:#555555;cursor:pointer;}
#store_params_batch_date{cursor:not-allowed;}
#store_params_batch_date.required{cursor:pointer;}
#qc_table_holder{display:none;}
#qc_table tr td:first-child{text-align: right;}
div fieldset{float:left;}
form fieldset legend{letter-spacing:0.03em; color:#555555;}
.formTable{
	padding-left:10px;
	margin-left:auto;
	margin-right:auto;
}
.validation-advice{font-size:0.9em;}
td{vertical-align:top;}
.field_associates, .formSelect{margin-right:15px;}
label.left, label.right{
	padding: 3px 0px 0px 0px;
}
label.left{text-align: left;}
label.right{text-align: right;}
.radio_button, .radio_button_label{float:left;padding:0px 10px 0px 4px;}
.radio_button{margin-top:5px;}
.radioLabel{
	font-weight:bold;
	margin-right:15px;
	white-space: nowrap;
}
.inactive{color: #555555;}

#restore_div{
	display:none;
	position:absolute;
	top:0px;
	left:0px;
	width:100%;
	height:100%;
	background: url(<%=asset_path "css/window_background_light.png"%>);
	z-index:90;
}

.noteContainer{
	float:left;
	width:100%;
	left:0px;
}

.analysisNotes{
	position:relative;
	float:none;
	left:0px;
	margin-left:740px;
	width:auto;
}

.formContainer{
	float:left;
	width:700px;
	margin-left:-100%;
}

#submission div{
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	padding:5px;
	border:1px solid #666666;
	width:95%;
}

#advanced_options_body input, #advanced_options_body select{
	display:inline;
	background:#ffff99;
	width:100px;
}

#american[readonly]{
	cursor:pointer;
}
#american[disabled]{
	cursor:not-allowed;
}

#generateHistogramButton{
	margin-left:70px;
	padding-right:12px;
	text-decoration:underline;
}

#privacyReminder{
	border:0px solid;
	font-size:1.2em;
	margin-bottom:40px;
}

input[type=submit], input[type=reset]{
	display:inline;
}

#resetButton{margin-left:85px;}

#normalizationMethod, #statMethod{	margin-top:0px;	}

#errorCorrection{
	margin-top:5px;
	border-top:1px solid #AAA;
	padding-top:5px;
}

input[type=radio]{display:inline;}

#flashMessageBackground .whiteFlash{
	width:75%;
	overflow: auto;
	background-color:#FFFFFF;
}

#flashMessageContainer ul{
	width:auto;
	margin:5px 25% 15px 25%;
	overflow:hidden;
	text-align: left;
}

#flashMessageContainer li{
	line-height:1.5em;
  float:left;
  display:inline;
  margin:auto;
  margin-left:5%;
  border-bottom:1px solid #ccc;
}
ul.double li  { width:45%;}
ul.triple li  { width:30%;}
#flashMessageContainer ul.triple li{
	margin-left: 3%;
}
#dup_experiments{
	width:100%;
	margin:5px 0px;
}
div.toggleRowDiv{
	padding-top:7px;
	display:none;
}
table.exp{
	margin:5px;
	font-weight:normal;
	font-style: normal;
}
table .qc-combo{cursor: pointer;}

#flashMessageContainer div.goodSets ul, #flashMessageContainer div.badSets ul{
	width:100%;
	margin:5px 0px 0px 0px;
}
div.badSets, div.goodSets{margin-top:10px;}